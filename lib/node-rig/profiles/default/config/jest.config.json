{
  "extends": "@rushstack/heft-jest-plugin/includes/jest-shared.config.json",
  "collectCoverage": true,
  "roots": ["<rootDir>/dist/__test__"],
  "coveragePathIgnorePatterns": ["<rootDir>/src/__test__"],
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "functions": 50,
      "lines": 50,
      "statements": 50
    }
  }
}
